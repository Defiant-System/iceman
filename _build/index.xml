</MenuBar></Head> <script><![CDATA[ const iceman = {
	init() {
		
	},
	dispatch(event) {
		switch (event.type) {
			case "keystroke":
				switch (event.char) {
					case "up":    this.gameManager.move(0); break;
					case "down":  this.gameManager.move(2); break;
					case "left":  this.gameManager.move(3); break;
					case "right": this.gameManager.move(1); break;
				}
				break;
		}
	}
};

window.exports = iceman;
 ]]></script><style><![CDATA[ .ant-window_[data-id="iceman"] .win-body_ content{position:absolute;top:0;left:0;right:0;bottom:0;border-radius:inherit}.ant-window_[data-id="iceman"] .win-body_ content .row{height:16px;padding:2px 0px 2px 0px;background:#000}.ant-window_[data-id="iceman"] .win-body_ content .row *{color:#fff;font-weight:bold;font-size:13px}.ant-window_[data-id="iceman"] .win-body_ content .row span{color:#F55;margin:0px 0px 0px 8px}.ant-window_[data-id="iceman"] .win-body_ content .row .level{float:left;width:60px;padding:0px 130px 0px 2px}.ant-window_[data-id="iceman"] .win-body_ content .row .diamond{float:left;width:100px;padding:0px 2px 0px 2px}.ant-window_[data-id="iceman"] .win-body_ content .row .score{float:right;width:80px;padding:0px 2px 0px 2px;text-align:right}.ant-window_[data-id="iceman"] .win-body_ content .rulers{position:relative;width:450px;height:450px;padding:15px 17px 17px 15px;background:#fff}.ant-window_[data-id="iceman"] .win-body_ content .rulers .rule-x{background:rgba(0,0,0,0.1);position:absolute;top:19px;left:0px;width:480px;height:30px}.ant-window_[data-id="iceman"] .win-body_ content .rulers .rule-y{background:rgba(0,0,0,0.1);position:absolute;top:0px;left:19px;width:30px;height:480px}.ant-window_[data-id="iceman"] .win-body_ content .rulers .aleft{background-position:-60px -90px;top:0px;left:0px;width:15px}.ant-window_[data-id="iceman"] .win-body_ content .rulers .aright{background-position:-90px -90px;top:0px;left:467px;width:15px}.ant-window_[data-id="iceman"] .win-body_ content .rulers .atop{background-position:-150px -90px;top:0px;left:0px;width:15px}.ant-window_[data-id="iceman"] .win-body_ content .rulers .abottom{background-position:-120px -90px;top:466px;left:0px;width:15px}.ant-window_[data-id="iceman"] .win-body_ content .board{position:relative;top:0;left:0;width:450px;height:450px;background:url(/app/ant/iceman/img/bg.png);border-radius:inherit}.ant-window_[data-id="iceman"] .win-body_ content .box{background